{
  "name": "fullstack-example-backend",
  "version": "0.2.0",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:open-api": "ts-node script/generate-open-api",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:sls": "nest build && sls offline",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.0.5",
    "@nestjs/config": "0.4.0",
    "@nestjs/core": "^7.0.5",
    "@nestjs/jwt": "^7.0.0",
    "@nestjs/passport": "^7.0.0",
    "@nestjs/platform-express": "^7.0.5",
    "@nestjs/swagger": "^4.4.0",
    "@nestjs/typeorm": "^7.0.0",
    "aws-lambda": "^1.0.5",
    "aws-serverless-express": "^3.3.6",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.11.1",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.4",
    "typeorm": "^0.2.24"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.2",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.5",
    "@types/aws-lambda": "^8.10.46",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.4",
    "@types/jest": "^25.1.4",
    "@types/node": "^13.9.3",
    "@types/passport-jwt": "^3.0.3",
    "@types/supertest": "^2.0.8",
    "jest": "^25.1.0",
    "mongodb-memory-server": "^6.4.1",
    "prettier": "^2.0.2",
    "serverless-dotenv-plugin": "^2.3.2",
    "serverless-offline": "^6.0.0",
    "supertest": "^4.0.2",
    "ts-jest": "^25.2.1",
    "ts-loader": "^6.2.2",
    "ts-node": "^8.8.1",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.1.0",
    "typescript": "^3.8.3"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
