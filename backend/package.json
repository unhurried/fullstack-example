{
  "name": "fullstack-example-backend",
  "version": "0.1.1",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:open-api": "ts-node script/generate-open-api",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:sls": "nest build && sls offline",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^6.11.1",
    "@nestjs/config": "0.1.0",
    "@nestjs/core": "^6.11.1",
    "@nestjs/platform-express": "^6.11.1",
    "@nestjs/swagger": "^4.2.0",
    "@nestjs/typeorm": "^6.2.0",
    "aws-lambda": "^1.0.5",
    "aws-serverless-express": "^3.3.6",
    "class-transformer": "^0.2.3",
    "class-validator": "^0.11.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.0",
    "rxjs": "^6.5.4",
    "swagger-ui-express": "^4.1.3",
    "typeorm": "^0.2.22"
  },
  "devDependencies": {
    "@nestjs/cli": "^6.14.1",
    "@nestjs/schematics": "^6.8.2",
    "@nestjs/testing": "^6.11.1",
    "@types/aws-lambda": "^8.10.40",
    "@types/aws-serverless-express": "^3.3.3",
    "@types/express": "^4.17.2",
    "@types/jest": "^24.9.1",
    "@types/node": "^13.5.0",
    "@types/supertest": "^2.0.8",
    "jest": "^25.1.0",
    "mongodb-memory-server": "^6.2.3",
    "prettier": "^1.19.1",
    "serverless-dotenv-plugin": "^2.1.1",
    "serverless-offline": "^5.12.1",
    "supertest": "^4.0.2",
    "ts-jest": "^25.0.0",
    "ts-loader": "^6.2.1",
    "ts-node": "^8.6.2",
    "tsconfig-paths": "^3.9.0",
    "tslint": "^6.0.0",
    "typescript": "^3.7.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
