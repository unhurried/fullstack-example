{
  "name": "fullstack-example-backend",
  "version": "0.5.1",
  "license": "MIT",
  "scripts": {
    "oidc-provider": "ts-node script/oidc-provider",
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:open-api": "ts-node script/generate-open-api",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "start:sls": "nest build && sls offline",
    "lint": "tslint -p tsconfig.json -c tslint.json",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "^7.6.18",
    "@nestjs/config": "1.1.6",
    "@nestjs/core": "^7.6.18",
    "@nestjs/graphql": "^7.9.8",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/passport": "^8.1.0",
    "@nestjs/platform-express": "^7.6.18",
    "@nestjs/swagger": "^4.8.2",
    "@nestjs/typeorm": "^7.1.5",
    "apollo-server-express": "^2.25.3",
    "aws-lambda": "^1.0.7",
    "aws-serverless-express": "^3.4.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "graphql": "^15.8.0",
    "graphql-tools": "^8.2.0",
    "jwks-rsa": "^2.0.5",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.6.7",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.41"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.0",
    "@nestjs/schematics": "^8.0.5",
    "@nestjs/testing": "^7.6.18",
    "@types/aws-lambda": "^8.10.90",
    "@types/aws-serverless-express": "^3.3.5",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/oidc-provider": "^7.8.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/supertest": "^2.0.11",
    "jest": "^27.4.7",
    "mongodb-memory-server": "^6.9.6",
    "oidc-provider": "^7.10.6",
    "prettier": "^2.5.1",
    "serverless-dotenv-plugin": "^3.12.2",
    "serverless-offline": "^8.3.1",
    "supertest": "^6.2.2",
    "ts-jest": "^27.1.3",
    "ts-loader": "^9.2.6",
    "ts-node": "^10.4.0",
    "tsconfig-paths": "^3.12.0",
    "tslint": "^6.1.3",
    "typescript": "^4.5.5"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "./coverage",
    "testEnvironment": "node"
  }
}
